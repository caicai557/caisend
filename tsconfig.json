{ 
  "compilerOptions": { 
    "target": "es6", 
    "module": "commonjs", 
    "esModuleInterop": true, 
    "strict": true, 
    "skipLibCheck": true, 
    "forceConsistentCasingInFileNames": true, 
    "moduleResolution": "node", 
    "resolveJsonModule": true, 
    "jsx": "react-jsx", 
    "types": ["node", "jest"] 
  }, 
  "include": ["**/*.ts", "**/*.tsx"], 
  "exclude": ["node_modules"] 
} 
