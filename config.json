    "hotkeys": {
      "show": "alt+`",
      "reload": "ctrl+alt+r",
      "toggle_send": "ctrl+alt+enter"
    }
  },
  "app": {
    "service_path": "C:/dev/reply-recosvc",
    "api_endpoints": {
