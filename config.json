      "phrases": "http://127.0.0.1:7788/phrases",
      "health": "http://127.0.0.1:7788/health",
      "metrics": "http://127.0.0.1:7788/metrics"
    },
    "security": {
      "token": "",
      "allow_origins": ["*"],
      "timeout": 10
    }
  },
  "cdp": {
    "host": "127.0.0.1",
    "port": null,
    "range": "9222-9333",
    "timeout": 0.5,
    "max_workers": 24
